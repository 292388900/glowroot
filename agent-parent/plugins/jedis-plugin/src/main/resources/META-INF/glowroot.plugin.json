{
  "name": "Jedis Plugin",
  "id": "jedis",
  "instrumentation": [
    {
      "className": "redis.clients.jedis.Connection",
      "methodName": "sendCommand",
      "methodParameterTypes": [
        "*",
        ".."
      ],
      "nestingGroup": "jedis",
      "captureKind": "trace-entry",
      "timerName": "jedis",
      "traceEntryMessageTemplate": "jedis {{this.host}}:{{this.port}} {{0}}"
    }
  ]
}
