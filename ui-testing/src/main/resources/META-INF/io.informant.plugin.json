{
  "name": "Ui Testing Plugin",
  "groupId": "io.informant",
  "artifactId": "informant-ui-testing",
  "version": "0.5-SNAPSHOT",
  "properties": [
    {
      "prompt": "Do something",
      "name": "oneName",
      "type": "boolean",
      "default": true,
      "description": "Do you want to do something?"
    },
    {
      "prompt": "Whatever you want",
      "name": "anotherName",
      "type": "string",
      "default": "coding",
      "description": "More help text here and more and more and more."
    },
    {
      "prompt": "For however long you want",
      "name": "thirdName",
      "type": "double",
      "description": "I'm quickly out of ideas."
    }
  ],
  "aspects": [
    "io.informant.testing.ui.NestableCallAspect",
    "io.informant.testing.ui.ExpensiveCallAspect"
  ]
}
