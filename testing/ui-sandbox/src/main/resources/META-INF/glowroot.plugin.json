{
  "name": "UI Sandbox Plugin",
  "id": "glowroot-ui-sandbox",
  "version": "0.5-SNAPSHOT",
  "transactionTypes": [
    "Sandbox",
    "Background"
  ],
  "traceCustomAttributes": [
    "My First Attribute",
    "Second",
    "A Very Long Attribute Value",
    "Another"
  ],
  "properties": [
    {
      "prompt": "Do something",
      "name": "oneName",
      "type": "boolean",
      "default": true,
      "description": "Do you want to do something?"
    },
    {
      "prompt": "Whatever you want",
      "name": "anotherName",
      "type": "string",
      "default": "coding",
      "description": "More help text here and more and more and more."
    },
    {
      "prompt": "For however long you want",
      "name": "thirdName",
      "type": "double",
      "description": "I'm quickly out of ideas."
    },
    {
      "prompt": "Lucky number",
      "name": "fourthName",
      "type": "double",
      "description": "Good luck."
    }
  ],
  "aspects": [
    "org.glowroot.sandbox.ui.NestableCallAspect",
    "org.glowroot.sandbox.ui.ExpensiveCallAspect",
    "org.glowroot.sandbox.ui.ExternalJvmMainAspect"
  ]
}
