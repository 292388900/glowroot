{
  "name": "Integration Tests Plugin",
  "id": "informant-integration-tests",
  "version": "0.5-SNAPSHOT",
  "properties": [
    {
      "prompt": "Starred grouping",
      "name": "starredGrouping",
      "type": "boolean"
    },
    {
      "prompt": "Alternate grouping",
      "name": "alternateGrouping",
      "type": "string"
    },
    {
      "prompt": "Has default val",
      "name": "hasDefaultVal",
      "type": "string",
      "default": "one"
    },
    {
      "prompt": "Capture span stack traces",
      "name": "captureSpanStackTraces",
      "type": "boolean"
    }
  ],
  "aspects": [
    "io.informant.tests.plugin.LevelOneAspect",
    "io.informant.tests.plugin.LevelTwoAspect",
    "io.informant.tests.plugin.LevelThreeAspect",
    "io.informant.tests.plugin.LogErrorAspect",
    "io.informant.tests.plugin.LogCauseAspect",
    "io.informant.tests.plugin.PauseAspect",
    "io.informant.tests.plugin.MatchAllTypesAspect"
  ]
}
